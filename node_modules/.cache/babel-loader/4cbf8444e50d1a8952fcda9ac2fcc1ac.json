\r\n    <div class=\"vx-row\">\r\n      <!-- LINE CHART -->\r\n      <div class=\"vx-col w-full md:w-2/3 mb-base\">\r\n        <vx-card title=\"Revenue\">\r\n          <template slot=\"actions\">\r\n            <feather-icon icon=\"SettingsIcon\" svgClasses=\"w-6 h-6 text-grey\"></feather-icon>\r\n          </template>\r\n          <div slot=\"no-body\" class=\"p-6 pb-0\">\r\n            <div class=\"flex\">\r\n              <div class=\"mr-6\">\r\n                <p class=\"mb-1 font-semibold\">This Month</p>\r\n                <p class=\"text-3xl text-success\">\r\n                  <sup class=\"text-base mr-1\">DH</sup>8 600\r\n                </p>\r\n              </div>\r\n              <div>\r\n                <p class=\"mb-1 font-semibold\">Last Month</p>\r\n                <p class=\"text-3xl\">\r\n                  <sup class=\"text-base mr-1\">DH</sup>7 300\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <vue-apex-charts\r\n              type=\"line\"\r\n              height=\"266\"\r\n              :options=\"analyticsData.revenueComparisonLine.chartOptions\"\r\n              :series=\"analyticsData.revenueComparisonLine.series\"\r\n            />\r\n          </div>\r\n        </vx-card>\r\n      </div>\r\n\r\n      <!-- RADIAL CHART -->\r\n      <div class=\"vx-col w-full md:w-1/3 mb-base\">\r\n        <vx-card title=\"Goal Overview\">\r\n          <template slot=\"actions\">\r\n            <feather-icon icon=\"HelpCircleIcon\" svgClasses=\"w-6 h-6 text-grey\"></feather-icon>\r\n          </template>\r\n\r\n          <!-- CHART -->\r\n          <template slot=\"no-body\">\r\n            <div class=\"mt-10\">\r\n              <vue-apex-charts\r\n                type=\"radialBar\"\r\n                height=\"266\"\r\n                :options=\"analyticsData.goalOverviewRadialBar.chartOptions\"\r\n                :series=\"analyticsData.goalOverviewRadialBar.series\"\r\n              />\r\n            </div>\r\n          </template>\r\n\r\n          <!-- DATA -->\r\n          <div class=\"flex justify-between text-center\" slot=\"no-body-bottom\">\r\n            <div\r\n              class=\"w-1/2 border border-solid border-grey-light border-r-0 border-b-0 border-l-0\"\r\n            >\r\n              <p class=\"mt-4\">Orders Completed</p>\r\n              <p class=\"mb-4 text-3xl font-semibold\">1 500</p>\r\n            </div>\r\n            <div class=\"w-1/2 border border-solid border-grey-light border-r-0 border-b-0\">\r\n              <p class=\"mt-4\">In sale</p>\r\n              <p class=\"mb-4 text-3xl font-semibold\">561</p>\r\n            </div>\r\n          </div>\r\n        </vx-card>\r\n      </div>\r\n     </div>\r\n      <div class=\"vx-row\">\r\n       <div class=\"vx-col w-full md:w-1/3 lg:w-1/3 xl:w-1/3 mb-base\">\r\n        <vx-card title=\"Users Statistics\">\r\n          <div\r\n            v-for=\"(browser, index) in analyticsData.browserAnalytics\"\r\n            :key=\"browser.id\"\r\n            :class=\"{'mt-4': index}\"\r\n          >\r\n            <div class=\"flex justify-between\">\r\n              <div class=\"flex flex-col\">\r\n                <span class=\"mb-1\">{{ browser.name }}</span>\r\n                <h4>{{ browser.ratio }}%</h4>\r\n              </div>\r\n              <div class=\"flex flex-col text-right\">\r\n                <span class=\"flex -mr-1\">\r\n                  <span class=\"mr-1\">{{ browser.comparedResult }}</span>\r\n                  <feather-icon\r\n                    :icon=\" browser.comparedResult < 0 ? 'ArrowDownIcon' : 'ArrowUpIcon'\"\r\n                    :svgClasses=\"[browser.comparedResult < 0 ? 'text-danger' : 'text-success'  ,'stroke-current h-4 w-4 mb-1 mr-1']\"\r\n                  ></feather-icon>\r\n                </span>\r\n                <span class=\"text-grey\">{{ browser.time | time(true) }}</span>\r\n              </div>\r\n            </div>\r\n            <vs-progress :percent=\"browser.ratio\"></vs-progress>\r\n          </div>\r\n        </vx-card>\r\n      </div>\r\n\r\n      <div class=\"vx-col w-full md:w-2/3 mb-base\">\r\n        <vx-card title=\"Client Retention\">\r\n          <div class=\"flex\">\r\n            <span class=\"flex items-center\">\r\n              <div class=\"h-3 w-3 rounded-full mr-1 bg-primary\"></div>\r\n              <span>New Clients</span>\r\n            </span>\r\n            <span class=\"flex items-center ml-4\">\r\n              <div class=\"h-3 w-3 rounded-full mr-1 bg-danger\"></div>\r\n              <span>Retained Clients</span>\r\n            </span>\r\n          </div>\r\n          <vue-apex-charts\r\n            type=\"bar\"\r\n            height=\"277\"\r\n            :options=\"analyticsData.clientRetentionBar.chartOptions\"\r\n            :series=\"analyticsData.clientRetentionBar.series\"\r\n          />\r\n        </vx-card>\r\n      </div>\r\n    </div>\r\n\r\n        <div class=\"vx-row  mb-base\">\r\n              <!-- CARD 9: DISPATCHED ORDERS -->\r\n              <div class=\"vx-col w-full \">\r\n                <vx-card title=\"Dispatched Orders\">\r\n                  <div slot=\"no-body\" class=\"mt-4\">\r\n                    <vs-table :data=\"users\">\r\n                      <template slot=\"thead\">\r\n                        <vs-th>ORDER NO.</vs-th>\r\n                        <vs-th>STATUS</vs-th>\r\n                        <vs-th>OPERATORS</vs-th>\r\n                        <vs-th>LOCATION</vs-th>\r\n                        <vs-th>DISTANCE</vs-th>\r\n                        <vs-th>START DATE</vs-th>\r\n                        <vs-th>EST DELIVERY DATE</vs-th>\r\n                      </template>\r\n\r\n                      <template slot-scope=\"{data}\">\r\n                        <vs-tr :key=\"indextr\" v-for=\"(tr, indextr) in data\">\r\n                          <vs-td :data=\"data[indextr].orderNo\">\r\n                            <span>#{{data[indextr].orderNo}}</span>\r\n                