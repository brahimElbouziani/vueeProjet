n  },\n  computed: {\n    currentPage: function currentPage() {\n      if (this.isMounted) {\n        return this.$refs.table.currentx;\n      }\n\n      return 0;\n    }\n  },\n  methods: {\n    getnamebuyer: function getnamebuyer(status) {\n      if (status.seller == true) return \"seller\";else if (status.bayer == true) return \"buyer\";\n      return \"primary\";\n    },\n    getOrderStatusColor: function getOrderStatusColor(status) {\n      if (status == 'on hold') return \"warning\";\n      if (status == 'delivered') return \"success\";\n      if (status == 'canceled') return \"danger\";\n      return \"primary\";\n    },\n    getPopularityColor: function getPopularityColor(num) {\n      if (num.seller == true) return \"success\";else if (num.bayer == true) return \"danger\";\n      return \"primary\";\n    }\n  },\n  created: function created() {\n    var thisIns = this;\n    this.$http.get('https://recyclers.herokuapp.com/api/admin/users').then(function (response) {\n      thisIns.users = response.data;\n    }).catch(function (error) {\n      thisIns.$vs.notify({\n        title: 'Error',\n        text: error,\n        color: 'danger',\n        iconPack: 'feather',\n        icon: 'icon-alert-circle'\n      });\n    });\n  },\n  mounted: function mounted() {\n    this.isMounted = true;\n  }\n};",{"version":3,"sources":["DataListListView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgHA,OAAA,iBAAA,MAAA,0BAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,iBAAA,EAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,YAAA,EAAA,CAHA;AAIA,MAAA,SAAA,EAAA,KAJA;AAKA,MAAA,iBAAA,EAAA;AALA,KAAA;AAOA,GAZA;AAaA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,eAAA,KAAA,KAAA,CAAA,KAAA,CAAA,QAAA;AACA;;AACA,aAAA,CAAA;AACA;AANA,GAbA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,wBACA,MADA,EACA;AACA,UAAA,MAAA,CAAA,MAAA,IAAA,IAAA,EAAA,OAAA,QAAA,CAAA,KACA,IAAA,MAAA,CAAA,KAAA,IAAA,IAAA,EAAA,OAAA,OAAA;AACA,aAAA,SAAA;AACA,KALA;AAMA,IAAA,mBANA,+BAMA,MANA,EAMA;AACA,UAAA,MAAA,IAAA,SAAA,EAAA,OAAA,SAAA;AACA,UAAA,MAAA,IAAA,WAAA,EAAA,OAAA,SAAA;AACA,UAAA,MAAA,IAAA,UAAA,EAAA,OAAA,QAAA;AACA,aAAA,SAAA;AACA,KAXA;AAYA,IAAA,kBAZA,8BAYA,GAZA,EAYA;AACA,UAAA,GAAA,CAAA,MAAA,IAAA,IAAA,EAAA,OAAA,SAAA,CAAA,KACA,IAAA,GAAA,CAAA,KAAA,IAAA,IAAA,EAAA,OAAA,QAAA;AACA,aAAA,SAAA;AACA;AAhBA,GArBA;AAuCA,EAAA,OAvCA,qBAuCA;AACA,QAAA,OAAA,GAAA,IAAA;AACA,SAAA,KAAA,CAAA,GAAA,CAAA,iDAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,MAAA,OAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AACA,QAAA,KAAA,EAAA,OADA;AAEA,QAAA,IAAA,EAAA,KAFA;AAGA,QAAA,KAAA,EAAA,QAHA;AAIA,QAAA,QAAA,EAAA,SAJA;AAKA,QAAA,IAAA,EAAA;AALA,OAAA;AAMA,KAXA;AAYA,GArDA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,SAAA,SAAA,GAAA,IAAA;AACA;AAxDA,CAAA","sourcesContent":["\r\n<template>\r\n  <div id=\"data-list-list-view\" class=\"data-list-container\">\r\n\r\n    <add-new-data-sidebar :isSidebarActive=\"addNewDataSidebar\" @closeSidebar=\"addNewDataSidebar = false\" />\r\n\r\n    <vs-table ref=\"table\" multiple v-model=\"selected\" pagination :max-items=\"itemsPerPage\" search :data=\"users\">\r\n\r\n      <div slot=\"header\" class=\"flex flex-wrap-reverse items-center flex-grow justify-between\">\r\n\r\n        <div class=\"flex flex-wrap-reverse items-center\">\r\n\r\n          <!-- ACTION - DROPDOWN -->\r\n          <vs-dropdown vs-trigger-click class=\"cursor-pointer mr-4 mb-4\">\r\n\r\n            <div class=\"p-4 shadow-drop rounded-lg d-theme-dark-bg cursor-pointer flex items-center justify-center text-lg font-medium w-32\">\r\n              <span class=\"mr-2\">Actions</span>\r\n              <feather-icon icon=\"ChevronDownIcon\" svgClasses=\"h-4 w-4\" />\r\n            </div>\r\n\r\n            <vs-dropdown-menu>\r\n\r\n              <vs-dropdown-item>\r\n                <span>Delete</span>\r\n              </vs-dropdown-item>\r\n\r\n              <vs-dropdown-item>\r\n                <span>Archive</span>\r\n              </vs-dropdown-item>\r\n              \r\n              \r\n            </vs-dropdown-menu>\r\n          </vs-dropdown>\r\n\r\n          \r\n        </div>\r\n\r\n        <!-- ITEMS PER PAGE -->\r\n        <vs-dropdown vs-trigger-click class=\"cursor-pointer mb-4 mr-4\">\r\n          <div class=\"p-4 border border-solid border-grey-light rounded-full d-theme-dark-bg cursor-pointer flex items-center justify-between font-medium\">\r\n            <span class=\"mr-2\">{{ currentPage * itemsPerPage - (itemsPerPage - 1) }} - {{ users.length - currentPage * itemsPerPage > 0 ? currentPage * itemsPerPage : users.length }} of {{ users.length }}</span>\r\n            <feather-icon icon=\"ChevronDownIcon\" svgClasses=\"h-4 w-4\" />\r\n          </div>\r\n            <vs-dropdown-menu>\r\n\r\n            <vs-dropdown-item @click=\"itemsPerPage=4\">\r\n              <span>4</span>\r\n            </vs-dropdown-item>\r\n            <vs-dropdown-item @click=\"itemsPerPage=10\">\r\n              <span>10</span>\r\n            </vs-dropdown-item>\r\n            <vs-dropdown-item @click=\"itemsPerPage=15\">\r\n              <span>15</span>\r\n            </vs-dropdown-item>\r\n            <vs-dropdown-item @click=\"itemsPerPage=20\">\r\n              <span>20</span>\r\n            </vs-dropdown-item>\r\n          </vs-dropdown-menu>\r\n        </vs-dropdown>\r\n      </div>\r\n\r\n      <template slot=\"thead\">\r\n         <vs-th sort-key=\"name\">Name</vs-th>\r\n        <vs-th sort-key=\"cin\">CIN</vs-th>\r\n        <vs-th sort-key=\"email\">Email</vs-th>\r\n       <vs-th sort-key=\"phone\">Phone</vs-th>\r\n        <vs-th sort-key=\"bayer\">Type</vs-th>\r\n         <vs-th sort-key=\"price\">Adresse</vs-th>\r\n      </template>\r\n\r\n        <template slot-scope=\"{data}\">\r\n          <tbody>\r\n            <vs-tr :data=\"tr\" :key=\"indextr\" v-for=\"(tr, indextr) in data\">\r\n\r\n           \r\n              <vs-td>\r\n                <p class=\"product-name font-medium\">{{ tr.name }}</p>\r\n              </vs-td>\r\n\r\n              <vs-td>\r\n                <p class=\"product-category\">{{tr.cin}}</p>\r\n              </vs-td>\r\n\r\n              <vs-td>\r\n                <p class=\"product-category\">{{ tr.email }}</p>\r\n              </vs-td>\r\n\r\n              <vs-td>\r\n                <p class=\"product-category\">+212 {{ tr.phone }}</p>\r\n              </vs-td>\r\n\r\n              <vs-td>\r\n                <vs-chip  :color=\"getPopularityColor(tr)\" \r\n                class=\"shadow-md\" >\r\n               {{ getnamebuyer(tr) }}\r\n                </vs-chip>\r\n              </vs-td>\r\n\r\n                \r\n\r\n              <vs-td>\r\n                <p class=\"product-price\">{{ tr.pos }}</p>\r\n              </vs-td>\r\n\r\n            </vs-tr>\r\n          </tbody>\r\n        </template>\r\n    </vs-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AddNewDataSidebar from '../AddNewDataSidebar.vue';\r\n\r\nexport default {\r\n  components: {\r\n    AddNewDataSidebar\r\n  },\r\n  data() {\r\n    return {\r\n      selected: [],\r\n      users: [],\r\n      itemsPerPage: 4,\r\n      isMounted: false,\r\n      addNewDataSidebar: false,\r\n    }\r\n  },\r\n  computed: {\r\n    currentPage() {\r\n      if(this.isMounted) {\r\n        return this.$refs.table.currentx\r\n      }\r\n      return 0\r\n    },\r\n  },\r\n  methods: {\r\n     getnamebuyer(status) {  \r\n      if(status.seller == true) return \"seller\"\r\n      else if(status.bayer == true) return \"buyer\"\r\n      return \"primary\"\r\n    },\r\n    getOrderStatusColor(status) {\r\n      if(status == 'on hold') return \"warning\"\r\n      if(status == 'delivered') return \"success\"\r\n      if(status == 'canceled') return \"danger\"\r\n      return \"primary\"\r\n    },\r\n    getPopularityColor(num) {\r\n      if(num.seller == true) return \"success\"\r\n      else if(num.bayer == true) return \"danger\"\r\n      return \"primary\"\r\n    } \r\n  },\r\n  created() {\r\n    const thisIns = this;\r\n    this.$http.get('https://recyclers.herokuapp.com/api/admin/users')\r\n      .then(function (response) {\r\n        thisIns.users =  response.data\r\n      })\r\n      .catch(function (error) {\r\n        thisIns.$vs.notify({\r\n          title:'Error',\r\n          text: error,\r\n          color:'danger',\r\n          iconPack: 'feather',\r\n          icon:'icon-alert-circle'})\r\n      });\r\n  },\r\n  mounted() {\r\n    this.isMounted = true;\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#data-list-list-view {\r\n  .vs-con-table {\r\n\r\n    .vs-table--header {\r\n      display: flex;\r\n      flex-wrap: wrap-reverse;\r\n      margin-left: 1.5rem;\r\n      margin-right: 1.5rem;\r\n      > span {\r\n        display: flex;\r\n        flex-grow: 1;\r\n      }\r\n\r\n      .vs-table--search{\r\n        padding-top: 0;\r\n\r\n        .vs-table--search-input {\r\n          padding: 0.9rem 2.5rem;\r\n          font-size: 1rem;\r\n\r\n          &+i {\r\n            left: 1rem;\r\n          }\r\n\r\n          &:focus+i {\r\n            left: 1rem;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    .vs-table {\r\n      border-collapse: separate;\r\n      border-spacing: 0 1.3rem;\r\n      padding: 0 1rem;\r\n\r\n      tr{\r\n          box-shadow: 0 4px 20px 0 rgba(0,0,0,.05);\r\n          td{\r\n            padding: 20px;\r\n            &:first-child{\r\n              border-top-left-radius: .5rem;\r\n              border-bottom-left-radius: .5rem;\r\n            }\r\n            &:last-child{\r\n              border-top-right-radius: .5rem;\r\n              border-bottom-right-radius: .5rem;\r\n            }\r\n          }\r\n          td.td-check{\r\n            padding: 20px !important;\r\n          }\r\n      }\r\n    }\r\n\r\n    .vs-table--thead{\r\n      th {\r\n        padding-top: 0;\r\n        padding-bottom: 0;\r\n\r\n        .vs-table-text{\r\n          text-transform: uppercase;\r\n          font-weight: 600;\r\n        }\r\n      }\r\n      th.td-check{\r\n        padding: 0 15px !important;\r\n      }\r\n      tr{\r\n        background: none;\r\n        box-shadow: none;\r\n      }\r\n    }\r\n\r\n    .vs-table--pagination {\r\n      justify-content: center;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/ui-elements/data-list/list-view"}]}